{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/business",
      "dest": "/api/business.js"
    },
    {
      "src": "/api/contact",
      "dest": "/api/contact.js"
    },
    {
      "src": "/api/incentives",
      "dest": "/api/incentives.js"
    },
    {
      "src": "/api/users/index",
      "dest": "/api/users/index.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/login",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/register",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/verify-token",
      "dest": "/api/verify-token.js"
    },
    {
      "src": "/api/verify-admin-code",
      "dest": "/api/auth/index.js"
    },
    {
      "src": "/api/business-search",
      "dest": "/api/business.js"
    },
    {
      "src": "/api/incentives/add",
      "dest": "/api/incentives.js"
    },
    {
      "src": "/api/users/password",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/users/update",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/users/profile",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/users/get",
      "dest": "/api/users.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}